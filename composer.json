{
  "name": "froyi/php-boilerplate",
  "description": "This is my boilerplate for every small project.",
  "type": "website",
  "keywords": [
    "edit these"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Froyi",
      "email": "ms2002@onlinehome.de"
    }
  ],
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^7",
    "phpunit/php-timer": "2.*",
    "tracy/tracy": "^2.5"
  },
  "require": {
    "php": "^7.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-mysqli": "*",
    "monolog/monolog": "^1.24",
    "twig/twig": "2.*",
    "ramsey/uuid": "3.8.*",
    "claviska/simpleimage": "3.*",
    "tinify/tinify": "1.*",
    "tecnickcom/tcpdf": "^6.2",
    "swiftmailer/swiftmailer": "6.*",
    "endroid/qr-code": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Project\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
